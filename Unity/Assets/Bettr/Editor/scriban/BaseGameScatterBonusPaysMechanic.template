{
    "Animations": [
        {
            "Filename": "{{machineName}}BaseGameSymbolSC",
            "AnimationStates": [
                {
                  "Name": "land_scatter",
                  "Speed": 1,
                  "IsLoop": false,
                  "IsDefault": false,
                  "Dopesheet": [
                    {
                      "Path": "Pivot/Quad",
                      "Type": "Transform",
                      "Property": "m_LocalScale.x",
                      "Keyframes": {
                        "Times": [0, 30, 60],
                        "Values": [1.5, 1, 1.5]
                      }
                    },
                    {
                      "Path": "Pivot/Quad",
                      "Type": "Transform",
                      "Property": "m_LocalScale.y",
                      "Keyframes": {
                        "Times": [0, 30, 60],
                        "Values": [1, 1, 1]
                      }
                    },
                    {
                      "Path": "Pivot/Quad",
                      "Type": "Transform",
                      "Property": "m_LocalScale.z",
                      "Keyframes": {
                        "Times": [0, 30, 60],
                        "Values": [1, 1, 1]
                      }
                    }
                  ]
                }
            ],
            "AnimatorTransitions": [
            ]
        }
    ],
    "TilePropertyAnimators": [
        {{- for tuple in scatterSymbolIndexesByReel }}
        {
            "PrefabName": "{{machineName}}BaseGameReel{{tuple.key}}",
            "PrefabIds": [
                {{- for symbolIndex in tuple.value }}
                {
                    "Prefix": "Symbol{{ symbolIndex }}",
                    "Index": {{ symbolIndex - 1 }},
                    "Id": "SC"
                }{{- if !for.last }},{{ end -}}
                {{- end -}}
            ],
            "AnimatorsProperty": [
                {{- for symbolIndex in tuple.value }}
                {
                    "Key": "SymbolScatterAnimation{{symbolIndex}}",
                    "State": "land_scatter",
                    "Id": "Symbol{{symbolIndex}}SC"
                }
                {{- if !for.last }},{{ end -}}
                {{- end -}}
            ]
        }{{- if !for.last }},{{ end -}}
        {{- end -}}
    ]
}